set nocompatible

set history=700

syntax on
filetype plugin on
filetype indent on

set autoread

set number

"set tabstop=4
"set shiftwidth=4
"set softtabstop=4
"set noexpandtab
"set smarttab
"set ai "Auto Indent
"set si " Smart Indent
set nowrap

set hlsearch
set incsearch

set hidden

colorscheme BusyBee

